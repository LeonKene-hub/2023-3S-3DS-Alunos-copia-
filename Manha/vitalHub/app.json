{
  "expo": {
    "name": "vitalhub",
    "slug": "vitalhub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "android": {
        "permissions": [
          "READ_EXTERNAL_STORAGE"
        ]
      }
    },
    "adaptiveIcon": {
      "foregroundImage": "./assets/logo.png",
      "backgroundColor": "#ffffff"
    }
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "plugins": [
    [
      "expo-camera",
      {
        "cameraPermission": "Permitir o VitalHub acessar a c√¢mera?",
        "enableMicrophonePermission": false
      }
    ],
    [
      "expo-notifications",
      {
        "icon": "./local/assets/logo.png",
        "color": "#FAFAFA"
      }
    ]
  ]
}